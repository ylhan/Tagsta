<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane prefHeight="392.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controller.ImageViewController">
   <children>
      <ScrollPane fx:id="scrollPane" fitToHeight="true" fitToWidth="true" pannable="true" prefHeight="393.0" prefWidth="656.0">
         <content>
            <VBox alignment="CENTER" fillWidth="false" styleClass="background">
               <children>
                  <HBox alignment="CENTER" fillHeight="false" styleClass="background">
                     <children>
                        <ImageView fx:id="imageView" preserveRatio="true" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </content>
      </ScrollPane>
      <AnchorPane>
         <children>
            <ButtonBar buttonMinWidth="40.0" buttonOrder="" layoutX="1.0" layoutY="130.0" prefHeight="31.0" prefWidth="68.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0">
               <buttons>
                  <Button fx:id="zoomIn" alignment="CENTER" contentDisplay="CENTER" disable="true" mnemonicParsing="false" onMouseClicked="#handleZoomIn" />
                  <Button fx:id="zoomOut" alignment="CENTER" contentDisplay="CENTER" disable="true" mnemonicParsing="false" onMouseClicked="#handleZoomOut" />
               </buttons>
            </ButtonBar>
         </children>
      </AnchorPane>
   </children>
</StackPane>
